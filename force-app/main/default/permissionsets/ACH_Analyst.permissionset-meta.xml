<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Analyst - Read and edit access to Funds, Investments, and Valuations for portfolio tracking and analysis. Limited access to investor-sensitive data (Investors, Commitments, Capital Calls, Distributions).</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>ACH Analyst</label>
    
    <!-- Apex Class Access - Read-only controllers for reporting -->
    <classAccesses>
        <apexClass>ACH_LPDataAccess</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>ACHDataGenerator</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    
    <!-- Fund Object - Full Access -->
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Fund__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Investor Object - Read Only -->
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Investor__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Commitment Object - Read Only -->
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Commitment__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Capital Call Object - Read Only -->
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Capital_Call__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Capital Call Line Object - Read Only -->
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Capital_Call_Line__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Distribution Object - Read Only -->
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Distribution__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Distribution Line Object - Read Only -->
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Distribution_Line__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Investment Object - Full Access (Portfolio Analysis) -->
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Investment__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Valuation Object - Full Access (Portfolio Analysis) -->
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>SK_ACH_Valuation__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Tab Visibility -->
    <tabSettings>
        <tab>SK_ACH_Fund__c</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    <tabSettings>
        <tab>SK_ACH_Investment__c</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    <tabSettings>
        <tab>SK_ACH_Valuation__c</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    <tabSettings>
        <tab>SK_ACH_Investor__c</tab>
        <visibility>Available</visibility>
    </tabSettings>
    <tabSettings>
        <tab>SK_ACH_Commitment__c</tab>
        <visibility>Available</visibility>
    </tabSettings>
    <tabSettings>
        <tab>SK_ACH_Capital_Call__c</tab>
        <visibility>Available</visibility>
    </tabSettings>
    <tabSettings>
        <tab>SK_ACH_Distribution__c</tab>
        <visibility>Available</visibility>
    </tabSettings>
</PermissionSet>
