<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Signature_Date_Must_Be_Before_Today</fullName>
    <active>true</active>
    <description>Signature date cannot be in the future</description>
    <errorConditionFormula>SK_ACH_Signature_Date__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>SK_ACH_Signature_Date__c</errorDisplayField>
    <errorMessage>Signature date cannot be in the future.</errorMessage>
</ValidationRule>
