<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fair_Value_Must_Be_Positive</fullName>
    <active>true</active>
    <description>Fair value must be greater than or equal to zero</description>
    <errorConditionFormula>SK_ACH_Fair_Value__c &lt; 0</errorConditionFormula>
    <errorMessage>Fair value cannot be negative.</errorMessage>
</ValidationRule>
