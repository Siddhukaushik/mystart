<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Due_Date_After_Call_Date</fullName>
    <active>true</active>
    <description>Due date must be after call date</description>
    <errorConditionFormula>SK_ACH_Due_Date__c &lt; SK_ACH_Call_Date__c</errorConditionFormula>
    <errorDisplayField>SK_ACH_Due_Date__c</errorDisplayField>
    <errorMessage>Due date must be on or after the call date.</errorMessage>
</ValidationRule>
