<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SK_ACH_Payment_Completion__c</fullName>
    <description>Percentage of capital call that has been paid</description>
    <formula>IF(SK_ACH_Total_Amount__c &gt; 0, 
    (SK_ACH_Total_Line_Paid__c / SK_ACH_Total_Amount__c) * 100, 
    0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Payment Completion (%)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
